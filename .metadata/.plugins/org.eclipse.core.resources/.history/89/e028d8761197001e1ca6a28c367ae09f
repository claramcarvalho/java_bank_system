package client;

import java.awt.EventQueue;

import javax.swing.JFrame;

public class FormDeposit {

	private JFrame frmDeposit;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					FormDeposit window = new FormDeposit();
					window.frmDeposit.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the application.
	 */
	public FormDeposit() {
		initialize();
	}

	/**
	 * Initialize the contents of the frame.
	 */
	private void initialize() {
		frmDeposit = new JFrame();
		frmDeposit.setTitle("Make a deposit");
		frmDeposit.setBounds(100, 100, 450, 300);
		frmDeposit.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frmDeposit.getContentPane().setLayout(null);
	}

}
