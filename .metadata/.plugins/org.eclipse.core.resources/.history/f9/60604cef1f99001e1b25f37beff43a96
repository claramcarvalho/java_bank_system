		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					FormViewTransactions window = new FormViewTransactions((Integer)null);
					window.frmViewTransactions.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
